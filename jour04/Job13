def supprimer_doublons(lst):
    liste_sans_doublons = []
    for element in lst:
        if element not in liste_sans_doublons:
            liste_sans_doublons.append(element)
    return liste_sans_doublons

# Liste initiale
liste_initiale = [10, 20, 30, 20, 10, 50, 60, 40, 80, 50, 40]

# Appel de la fonction pour supprimer les doublons
nouvelle_liste = supprimer_doublons(liste_initiale)

# Affichage du r√©sultat
print("Liste initiale:", liste_initiale)
print("Liste sans doublons:", nouvelle_liste)
